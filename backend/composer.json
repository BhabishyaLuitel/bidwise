{
    "name": "bidwise/auction-platform",
    "description": "A secure and efficient online auction platform.",
    "type": "project",
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "laravel/sanctum": "^2.0",
        "laravel/tinker": "^2.0",
        "fideloper/proxy": "^4.4",
        "guzzlehttp/guzzle": "^7.0",
        "stripe/stripe-php": "^9.0",
        "beyondcode/laravel-websockets": "^1.12",
        "laravel/socialite": "^5.2",
        "spatie/laravel-permission": "^5.5",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "mockery/mockery": "^1.4",
        "laravel/dusk": "^6.19"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "php artisan package:discover --ansi"
        ],
        "test": "phpunit"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}